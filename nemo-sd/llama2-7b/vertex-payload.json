            "acceleratorCount": 8
          },  
          "replicaCount": "1",
          "containerSpec": {
            "imageUri": "classicboyir/nemo:03",
            "command": [
                "sh", "-c"
            ],
            "args": [
              "git clone https://github.com/hosseinsarshar/a3-bandwidth-test.git && chmod +x ./a3-bandwidth-test/a3-mega/vertex/nemo/job/job_template.sh && ./a3-bandwidth-test/a3-mega/vertex/nemo/job/job_template.sh"
            ],
            "env": [
                {
                  "name": "LD_LIBRARY_PATH",
                  "value": "/usr/local/nvidia/lib64"
                },
                {
                  "name": "NNODES",
